<%= form_for(@user) do |f| %> 
    <%=f.label :name, "Name"%>
    <%= f.text_field :name %> 
    <%=f.label :email %>
    <%= f.text_field :email %>
    <%=f.label :username, "username" %>
    <%= f.text_field :username %> 
    <%=f.label :password, 'password' %> 
    <%= f.password_field :password_digest %>
    <%= f.password_field :password_confirmation %> 
    
    <%= f.submit "Create User" %> 
    
    <%end%> 